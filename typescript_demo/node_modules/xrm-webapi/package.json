{
  "name": "xrm-webapi",
  "version": "7.1.3",
  "description": "Dynamics 365 Web Api TypeScript Module",
  "main": "./dist/xrm-webapi.js",
  "types": "./dist/xrm-webapi.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
